{
  "name": "care-quality-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx",
    "deploy-migrations": "prisma migrate deploy --preview-feature",
    "openapi:generate": "node openapi-generator.js",
    "docz:dev": "docz dev",
    "docz:build": "mkdir -p .docz && cp docz-rsuite-ssr-loader.js .docz/ && docz build",
    "docz:serve": "docz serve",
    "test:frontend": "jest --testPathPattern=components* --config=jest-frontend.config.js",
    "test:backend": "env-cmd -f .env.test jest --testPathPattern=test/api* --runInBand",
    "pretest:e2e": "env-cmd -f .env.test npm run build",
    "test:e2e": "env-cmd -f .env.test jest --testPathPattern=test/end-to-end* --runInBand --config=jest-e2e.config.js"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@prisma/client": "^2.18.0",
    "@zeit/next-less": "^1.0.1",
    "chart.js": "^2.9.4",
    "env-cmd": "^10.1.0",
    "less": "^4.1.1",
    "next": "^10.0.7",
    "next-auth": "^3.7.0",
    "next-transpile-modules": "^6.3.0",
    "node-fetch": "^2.6.1",
    "openapi-response-validator": "^7.4.0",
    "pg": "^8.5.1",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-bootstrap": "^2.0.3",
    "react-chartjs-2": "^2.11.1",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dom": "16.13.1",
    "react-wordcloud": "^1.2.7",
    "rsuite": "^4.9.2",
    "styled-components": "^5.3.3",
    "swagger-jsdoc": "^6.0.8",
    "swr": "^0.3.11"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-classes": "^7.13.0",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "babel-jest": "^24.5.0",
    "docz": "^2.3.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "expect-puppeteer": "^4.4.0",
    "formik": "^2.2.9",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "json-schema-ref-parser": "^9.0.7",
    "next-test-api-route-handler": "^1.2.20",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "prisma": "^2.18.0",
    "puppeteer": "^8.0.0",
    "react-error-overlay": "^6.0.9",
    "react-router": "^6.0.2",
    "react-test-renderer": "^16.8.4",
    "spawnd": "^4.4.0",
    "webpack": "^4.46.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint"
    }
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ]
  }
}
